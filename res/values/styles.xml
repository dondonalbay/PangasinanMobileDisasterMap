<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
     <style name="Theme.Actionbarlayout" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarItemBackground">@drawable/selectable_background_actionbarlayout</item>
        <item name="android:popupMenuStyle">@style/PopupMenu.Actionbarlayout</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Actionbarlayout</item>
        <item name="android:actionBarTabStyle">@style/ActionBarTabStyle.Actionbarlayout</item>
        <item name="android:actionDropDownStyle">@style/DropDownNav.Actionbarlayout</item>
        <item name="android:actionBarStyle">@style/ActionBar.Solid.Actionbarlayout</item>
        <item name="android:actionModeBackground">@drawable/cab_background_top_actionbarlayout</item>
        <item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_actionbarlayout</item>
        <item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Actionbarlayout</item>
        
        
    </style>

</resources>
